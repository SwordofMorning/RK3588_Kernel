		regulator-always-on;
		enable-active-high;
		gpio = <&gpio0 RK_PC4 GPIO_ACTIVE_HIGH>;
	};

	/* HGD 1v8 */
	vcc_1v8_en: vcc-1v8-en {
		compatible = "regulator-fixed";
