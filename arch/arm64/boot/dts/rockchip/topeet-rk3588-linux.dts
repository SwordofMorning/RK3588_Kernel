    adc_keys: adc-keys {
        compatible = "adc-keys";
        io-channels = <&saradc 1>;
        io-channel-names = "buttons";
        keyup-threshold-microvolt = <1800000>;
        poll-interval = <100>;

        KEY_F1 {
            label = "KEY_F1";
            linux,code = <KEY_F1>;
			press-threshold-microvolt = <18000>;
        };

		KEY_F2 {
            label = "KEY_F2";
            linux,code = <KEY_F2>;
			press-threshold-microvolt = <600000>;
        };

		KEY_F3 {
            label = "KEY_F3";
            linux,code = <KEY_F3>;
			press-threshold-microvolt = <1000000>;
        };
    };

	gpio_keys: gpio-keys {
		compatible = "gpio-keys";

		KEY_F4 {
            label = "KEY_F4";
            gpios = <&gpio1 RK_PC3 GPIO_ACTIVE_LOW>;
            linux,code = <KEY_F4>;
        };

		KEY_F5 {
            label = "KEY_F5";
            gpios = <&gpio1 RK_PC5 GPIO_ACTIVE_LOW>;
            linux,code = <KEY_F5>;
        };

		KEY_C {
            label = "KEY_C";
            gpios = <&gpio1 RK_PA2 GPIO_ACTIVE_LOW>;
            linux,code = <KEY_0>;
        };

		KEY_S {
            label = "KEY_C";
            gpios = <&gpio2 RK_PC0 GPIO_ACTIVE_LOW>;
            linux,code = <KEY_1>;
        };

		KEY_A {
            label = "KEY_A";
            gpios = <&gpio1 RK_PD2 GPIO_ACTIVE_LOW>;
            linux,code = <KEY_2>;
        };

		KEY_UP {
            label = "KEY_UP";
            gpios = <&gpio2 RK_PC5 GPIO_ACTIVE_LOW>;
            linux,code = <KEY_3>;
        };

		KEY_LEFT {
            label = "KEY_LEFT";
            gpios = <&gpio4 RK_PC6 GPIO_ACTIVE_LOW>;
            linux,code = <KEY_4>;
        };

		KEY_RIGHT {
            label = "KEY_RIGHT";
            gpios = <&gpio0 RK_PA0 GPIO_ACTIVE_LOW>;
            linux,code = <KEY_5>;
        };

		KEY_DOWN {
            label = "KEY_DOWN";
            gpios = <&gpio2 RK_PC2 GPIO_ACTIVE_LOW>;
